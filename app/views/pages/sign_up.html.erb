							<li class="submenu">
								<a href="#">Inventory</a>
								<ul>
									<li><a href="/inventory">Current Stock</a></li>
									<li><a href="/build">Design Your Own</a></li>
									<li><a href="/lineup">View the Lineup</a></li>
								</ul>
							</li>
							<li class="submenu">
								<a href="/services">Services</a>
								<ul>
									<li><a href="/schedule-test">Test Drives</a></li>
									<li><a href="/schedule-sales">Sales Appointments</a></li>
									<li><a href="/services">View All</a></li>
								</ul>
							</li>
							<li><a href="/contact-us">Contact Us</a>
							<% if logged_in? %>
							<li class="submenu">
                  				<a href="#">My Account</a>
                  				<ul>
                   	 				<li><a href="/users">All Users</a></li>
					                <li><%= link_to "Profile", current_user %></li>
					                <li><%= link_to "Settings", edit_user_path(current_user) %></li>
					                <li class="divider"></li>
					                <li><%= link_to "Log out", logout_path, method: "delete" %></li>
					            </ul>
					        </li>
					        <% else %>
								<li><a href="/sign-up" class="button special">Sign Up</a></li>
								<li><a href="/login" class="button special">Log In</a></li>
							<% end %>
						</ul>
					</nav>
				</header>

			<!-- Main -->
				<article id="main">

					<header class="special container">
						<span class="icon fa-calendar"></span>
						<h2>Schedule A <strong>Test Drive</strong></h2>
						<!--<p>Where that in the center faces the nameless horrors alone.</p>-->
					</header>

					<!-- One -->
						<section class="wrapper style4 container">

							<!-- Content -->
								<div class="content">
									<section>
										<!--<a href="#" class="image featured"><img src="assets/pic04.jpg" alt="" /></a>-->
										<header>
											<h3>Enter Your Information to Schedule</h3>
										</header>
										<%= form_for(@user) do |f| %>
									      <%= f.label :name %>
									      <%= f.text_field :name %>

									      <%= f.label :email %>
									      <%= f.email_field :email %>

									      <%= f.label :password %>
									      <%= f.password_field :password %>

									      <%= f.label :password_confirmation, "Confirmation" %>
									      <%= f.password_field :password_confirmation %>

									      <%= f.submit "Create my account", class: "btn btn-primary" %>
									    <% end %>
									</section>
								</div>
						</section>

					<!-- Two -->
						<section class="wrapper style1 container special">
							<div class="row">
								<div class="4u 12u(narrower)">

									<section>
